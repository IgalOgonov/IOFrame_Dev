{
	"options":{
		"override":false,
		"update":false
	},
	"attributes":{
		"title":"Articles",
		"id":"articles",
		"path":"cp/articles.php"
	},
	"template":"pages/base.txt",
	"variables":{
		"HAS_WRAPPER":true,
		"IS_CONTROL_PANEL":true
	},
	"items":{
		"js":[
			"ext/SimpleMDE/simplemde.min.js",
			"mixins/sourceURL.js",
			"mixins/eventHubManager.js",
			"components/searchList.js",
			{
				"path":"components/articles/articlesEditor.js",
				"create":true,
				"template":"js/components/viewUpdateCreateComponent.txt",
				"variables":{
				  "COMPONENT_NAME":"articles-editor",
				  "API_URL":"api/articles",
				  "MIXINS":"sourceURL,eventHubManager,IOFrameCommons",
				  "PASS_ID":false,
				  "PASS_ITEM":true
				}
			},
			"modules/CPMenu.js",
			{
				"path":"modules/articles.js",
				"create":true,
				"template":"js/modules/searchListAppCustomSet.txt",
				"variables":{
				  "VARIABLE_NAME":"articles",
				  "ELEMENT_NAME":"articles",
				  "VUE_NAME":"articles",
				  "API_URL":"api/articles",
				  "SEARCH_TITLE":"Browsing Articles",
				  "EDIT_TITLE":"Editing User",
				  "MIXINS":"sourceURL,eventHubManager,IOFrameCommons"
				}
			}
		],
		"css":[
				"ext/SimpleMDE/font-awesome.min.css",
				"ext/SimpleMDE/simplemde.min.css",
				"cp.css",
				"components/searchList.css",
				{
                  "path":"components/articles/articlesEditor.css",
                  "create":true,
				  "template":"css/components/viewUpdateCreateComponent.txt",
				  "variables":{
					"COMPONENT_NAME":"articles-editor"
				  }
				},
				{
					"path":"modules/articles.css",
					"create":true,
                  "template":"css/modules/searchListAppCustomSet.txt",
                  "variables":{
                    "ELEMENT_NAME":"articles",
                    "SEARCH_LIST_COMPACT_MAX_WIDTH":"800",
                    "HAS_TYPES":false,
                    "HAS_MODES":true
                  }
				},
				"modules/CPMenu.css"
		],
		"templates":[
			{
				"path":"modules/CPMenu.php",
				"create":false
			},
			{
				"path":"modules/articles.php",
				"create":true,
				"template":"templates/modules/baseAppModule.txt",
				"variables":{
				  "ELEMENT_NAME":"articles",
				  "HAS_TYPES":false,
				  "IDENTIFIER_PARAM":"items[selected].id",
				  "API_GET_ACTION":"getArticles",
				  "COMPONENT_NAME":"articles-editor",
				  "PASS_ID":false,
				  "PASS_ITEM":true,
				  "API_CREATE_ACTION":"",
				  "API_UPDATE_ACTION":"updateUser"
				}
			}
		]
	}
}